{
  "name": "prebuild-install",
  "version": "4.0.0",
  "description": "A command line tool to easily install prebuilt binaries for multiple version of node/iojs on a specific platform",
  "scripts": {
    "test": "verify-travis-appveyor && tape test/*-test.js && npm run audit && npm run lint",
    "audit": "nsp check",
    "lint": "standard"
  },
  "keywords": [
    "prebuilt",
    "binaries",
    "native",
    "addon",
    "module",
    "c",
    "c++",
    "bindings",
    "devops"
  ],
  "dependencies": {
    "detect-libc": "^1.0.3",
    "expand-template": "^1.0.2",
    "github-from-package": "0.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-abi": "^2.2.0",
    "noop-logger": "^0.1.1",
    "npmlog": "^4.0.1",
    "os-homedir": "^1.0.1",
    "pump": "^2.0.1",
    "rc": "^1.1.6",
    "simple-get": "^2.7.0",
    "tar-fs": "^1.13.0",
    "tunnel-agent": "^0.6.0",
    "which-pm-runs": "^1.0.0"
  },
  "devDependencies": {
    "a-native-module": "^1.0.0",
    "nock": "^9.2.5",
    "nsp": "^2.3.0",
